F = 50;
t = 0:0.001:0.2;      %pasul de variaţie a variabilei t
s = 2*sin(2*pi*F*t);
plot(t,s,'.-'),xlabel('Timp [s]'),grid
>> hold on        %suprapune cel de-al doilea grafic peste primul
>> F = 20;
t = 0:0.001:0.2;        %pasul de variaţie a variabilei t
s = 2*cos(2*pi*F*t);
plot(t,s,'r.-'),xlabel('Timp [s]'),grid        %grafic cu linie rosie
>> hold off
